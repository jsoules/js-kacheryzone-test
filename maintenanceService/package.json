{
  "name": "kachery-cloud-maintenance-service",
  "version": "0.1.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "description": "Kachery cloud maintenance service",
  "bin": {
    "kachery-cloud-maintenance": "dist/cli.js"
  },
  "scripts": {
    "do-all": "npm run process-log-items && npm run download-logs && npm run analyze-logs && npm run upload-usage",
    "process-log-items": "ts-node ./src/processLogItems.ts",
    "download-logs": "ts-node ./src/downloadLogs.ts",
    "analyze-logs": "python python/analyze_logs.py",
    "upload-usage": "ts-node ./src/uploadUsage.ts",
    "-prod-example": "tsc && node dist/maintenanceService/src/cli.js",
    "-onetime-migrate-projects": "ts-node ./src/migrateProjects.ts",
    "-onetime-migrate-clients": "ts-node ./src/migrateClients.ts",
    "-onetime-move-logs-to-admin-bucket": "ts-node ./src/moveLogsToAdminBucket.ts",
    "-onetime-consolidate-logs": "ts-node ./src/consolidateLogs.ts",
    "-old-process-uploads": "ts-node ./src/processUploads.ts",
    "build": "tsc"
  },
  "author": "Jeremy Magland",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/firestore": "^6.4.1",
    "aws-sdk": "^2.1236.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "ts-node": "^10.7.0"
  }
}
