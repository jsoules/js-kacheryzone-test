{
  "name": "kachery-cloud-maintenance-service",
  "version": "0.1.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "description": "Kachery cloud maintenance service",
  "bin": {
    "kachery-cloud-maintenance": "dist/cli.js"
  },
  "scripts": {
    "-prod-example": "tsc && node dist/maintenanceService/src/cli.js",
    "migrate-projects": "ts-node ./src/migrateProjects.ts",
    "process-log-items": "ts-node ./src/processLogItems.ts",
    "download-logs": "ts-node ./src/downloadLogs.ts",
    "process-uploads": "ts-node ./src/processUploads.ts",
    "migrate-clients": "ts-node ./src/migrateClients.ts",
    "move-logs-to-admin-bucket": "ts-node ./src/moveLogsToAdminBucket.ts",
    "move-clients-to-admin-bucket": "ts-node ./src/moveClientsToAdminBucket.ts",
    "build": "tsc"
  },
  "author": "Jeremy Magland",
  "license": "Apache-2.0",
  "dependencies": {
    "aws-sdk": "^2.1236.0",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "ts-node": "^10.7.0"
  }
}
